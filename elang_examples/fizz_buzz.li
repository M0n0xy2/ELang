extern fn printf(*char, ..) -> int;

fn main() -> int {
    let counter = 1;
    while counter <= 100 {
        if counter % 15 == 0 {
            printf("fizzbuzz\n");
        } else if counter % 5 == 0 {
            printf("buzz\n");
        } else if counter % 3 == 0 {
            printf("fizz\n");
        } else {
            printf("%d\n", counter);
        }
        counter = counter + 1;
    }

    return 0;
}
