extern fn printf(*char, ..) -> int;

fn main() -> int {
    for counter = 1; counter <= 100; counter = counter + 1 {
        if counter % 15 == 0 {
            printf("fizzbuzz\n");
        } else if counter % 5 == 0 {
            printf("buzz\n");
        } else if counter % 3 == 0 {
            printf("fizz\n");
        } else {
            printf("%d\n", counter);
        }
    }

    return 0;
}
