fn neg_square(x: double) -> double {
    let y = x * x;
    return -y;
}

fn test_if(x: int) -> int {
    if x < 0 {
        return -x;
    } else if x > 0 {
        x + 2;
    } else if x == 12 {
        x;
        loop {break;}
    } else if x {
        x;
    } else {
        x + 4;
    }
}

fn test_loop(x: double) -> double {
    loop {
        return x;
        break;
        continue;
    }
}

fn test_while(x: int) {
    while x >= 0 {
        x = x - 1;
    }
}

fn test_bool(b: bool) -> bool {
    return !(!b);
}

fn test_ptr(ptr: *int) -> int {
    *ptr = 12;
    return *(&(*ptr));
}

fn print(_:double) {}

fn main() {
    print(neg_square(2.0 * 6.3));
}
